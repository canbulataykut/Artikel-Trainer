@charset "utf-8";

* { margin: 0px; padding: 0px; box-sizing: border-box; }

body { font-family: "Arial Black", Arial, sans-serif; background: linear-gradient(135deg, rgb(135, 206, 235) 0%, rgb(152, 216, 200) 100%); min-height: 100vh; display: flex; justify-content: center; align-items: center; padding: 10px; }

.container { max-width: 500px; width: 100%; background: rgb(139, 115, 85); border: 8px solid rgb(101, 67, 33); box-shadow: rgba(0, 0, 0, 0.5) 0px 10px 30px; border-radius: 10px; overflow: hidden; }

.header { background: rgb(44, 95, 45); color: white; padding: 20px; text-align: center; border-bottom: 5px solid rgb(26, 58, 27); text-shadow: rgba(0, 0, 0, 0.3) 3px 3px 0px; }

.header h1 { font-size: 28px; margin-bottom: 10px; }

.stats { display: flex; justify-content: space-around; background: rgb(92, 64, 51); padding: 15px; color: white; font-size: 18px; border-bottom: 4px solid rgb(61, 43, 35); text-shadow: rgba(0, 0, 0, 0.3) 2px 2px 0px; }

.game-area { background: rgb(160, 130, 109); padding: 30px 20px; min-height: 450px; display: flex; flex-direction: column; justify-content: center; align-items: center; }

.word-display { background: rgb(101, 67, 33); border: 6px solid rgb(61, 43, 35); padding: 30px; border-radius: 10px; margin-bottom: 30px; width: 100%; text-align: center; box-shadow: rgba(0, 0, 0, 0.3) 0px 5px 15px; }

.word { font-size: 42px; color: rgb(255, 215, 0); font-weight: bold; text-shadow: rgba(0, 0, 0, 0.5) 3px 3px 0px; }

.buttons { display: flex; flex-direction: column; gap: 15px; width: 100%; }

.btn { font-family: "Arial Black", Arial, sans-serif; font-size: 32px; padding: 20px; border: 6px solid rgb(0, 0, 0); border-radius: 8px; cursor: pointer; font-weight: bold; text-transform: uppercase; box-shadow: rgb(0, 0, 0) 0px 6px 0px; transition: 0.1s; text-shadow: rgba(0, 0, 0, 0.3) 2px 2px 0px; }

.btn:active { transform: translateY(4px); box-shadow: rgb(0, 0, 0) 0px 2px 0px; }

.btn-der { background: rgb(93, 173, 226); color: white; }

.btn-die { background: rgb(236, 112, 99); color: white; }

.btn-das { background: rgb(88, 214, 141); color: white; }

.btn-start { background: rgb(243, 156, 18); color: white; font-size: 28px; margin-top: 20px; }

.btn-count { background: rgb(52, 152, 219); color: white; font-size: 24px; margin: 5px; padding: 10px 20px; display: inline-block; border: 4px solid rgb(0, 0, 0); border-radius: 5px; cursor: pointer; }

.btn-count.active { background: rgb(39, 174, 96); border-color: rgb(30, 132, 73); }

.timer { font-size: 24px; color: rgb(255, 215, 0); margin-bottom: 20px; font-weight: bold; text-shadow: rgba(0, 0, 0, 0.5) 2px 2px 0px; }

.feedback { font-size: 28px; font-weight: bold; padding: 20px; margin: 20px 0px; border-radius: 10px; text-align: center; border: 5px solid; text-shadow: rgba(0, 0, 0, 0.3) 2px 2px 0px; }

.correct { background: rgb(88, 214, 141); color: white; border-color: rgb(46, 204, 113); }

.incorrect { background: rgb(236, 112, 99); color: white; border-color: rgb(231, 76, 60); }

.result { text-align: center; color: white; font-size: 20px; line-height: 1.8; }

.grade { font-size: 72px; color: rgb(255, 215, 0); margin: 20px 0px; text-shadow: rgba(0, 0, 0, 0.5) 4px 4px 0px; }

.progress-info { background: rgb(101, 67, 33); border: 4px solid rgb(61, 43, 35); padding: 15px; margin: 20px 0px; border-radius: 10px; font-size: 22px; width: 100%; }

.progress-bar { background: rgb(61, 43, 35); height: 30px; border-radius: 15px; margin-top: 10px; overflow: hidden; }

.progress-fill { background: linear-gradient(90deg, rgb(46, 204, 113), rgb(88, 214, 141)); height: 100%; transition: width 0.3s; text-align: center; color: white; font-weight: bold; line-height: 30px; font-size: 14px; }
